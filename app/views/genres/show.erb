<h4>Genre: <%= @genre.name %></h1>
	<p>
		Songs: 
		<ul>
			<% @genre.songs.map do |song| %>
				<li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
			<% end %>
		</ul>
		<br>		
		Artists:
		<ul>
			<% @genre.artists.map do |artist| %>
				<li><a href="/artists/<%= artist.slug %>"><%= artist.name %></a></li>
			<% end %>
		</ul>
	</p>